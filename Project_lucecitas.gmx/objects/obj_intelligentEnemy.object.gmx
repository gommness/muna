    tiempo=tiempo-1;
}else{
    /// Calcula el path optimo hacia el jugador
    NoCol=true;
    if (point_distance(x, y, obl_player1.x, obl_player1.y) &lt; intRatioSight) { 
        path = path_add();
        mp_potential_path_object(path, obl_player1.x, obl_player1.y, intSpeed, 4, obj_cube);
        path_start(path, intSpeed, path_action_stop, true);
    } else {
        path_end();
    }
}

if (self.x &gt; obl_player1.x){
    self.image_yscale = -1;
    self.image_angle = point_direction(self.x, self.y, obl_player1.x, obl_player1.y);
} else {
    self.image_yscale = 1;
    self.image_angle = point_direction(self.x, self.y, obl_player1.x, obl_player1.y);

}

